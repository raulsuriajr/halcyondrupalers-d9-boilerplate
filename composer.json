{
    "name": "halcyondrupalers/d8-boilerplate",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Halcyon Digital Media Inc.",
            "homepage": "https://www.drupal.org/halcyon-digital-media-design-inc",
            "role": "Maintainer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/advagg": "^4.1",
        "drupal/back_to_top": "^2.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/bamboo_twig": "^5.0@alpha",
        "drupal/block_class": "^1.3",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/coffee": "^1.2",
        "drupal/config_perms": "^2.0",
        "drupal/console": "^1.9",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/db_maintenance": "^2.0",
        "drupal/devel": "^4.1",
        "drupal/editor_advanced_image": "^2.0@beta",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/image_effects": "^3.1",
        "drupal/imageapi_optimize": "^4.0@beta",
        "drupal/imageapi_optimize_resmushit": "^2.0@beta",
        "drupal/metatag": "^1.16",
        "drupal/module_filter": "^3.2",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/r4032login": "^2.1",
        "drupal/recaptcha": "^3.0",
        "drupal/simple_sitemap": "^3.10",
        "drupal/swiftmailer": "^2.0",
        "drupal/twig_extensions": "^2.0",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.1",
        "drupal/webform": "^6.0",
        "drush/drush": "^10.6",
        "guzzlehttp/guzzle": "6.5.5",
        "mobiledetect/mobiledetectlib": "^2.8",
        "symfony/var-dumper": "5.3.0",
        "twig/twig": "2.14.6",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "files": ["load.environment.php"]
    },
    "extra": {
        "drupal-scaffold": {
            "file-mapping": {
                "[web-root]/README.md": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/sites/default/default.settings.php": false,
                "[web-root]/.htaccess": false
            }
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "core": ["type:drupal-core"],
            "libraries/{$name}": ["type:drupal-library"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/bootstrap_sass": {
                "Fixes unexpected error in user edit" : "https://www.drupal.org/files/issues/2018-10-18/bootstrap_sass-fix-missing-file-library-2986370-13-d8.patch"
            }
        }
    }
}
